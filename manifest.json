{
  "manifest_version" :2,
  "name" : "Personalized New Tab",
  "description" :"Customize new tab experience to stay update on favorite news",
  "version" :"1.0",
  "browser_action" : {
    "default_title" : "Personalized New Tab"
  },
  "background": {
     "persistent": false,
     "scripts": [ "app.js" ]
  },
  "chrome_url_overrides": {
      "newtab": "index.html"
   },

   
   "offline_enabled": true

}
